on root:text:*:#:{
  if ($1 == .say) { .msg $chan $2- }
  if ($1 == .join) { .join $2- }
  if ($1 == .part) { .part $chan 12B12y12e .1 . . }
  if ($1 == .nick) { .msg $chan 12Me voy a Cambiar el nick a ->> $2-  | nick $2- }
  if ($1 == .quit) { .quit $2- }
  if ($1 == .kick) { .kick $CHAN $2- }
  if ($1 == .msg) { .msg $2 $3- }
  if ($1 == .anti) { .nick AntiMp3 }
  if ($1 == .op) { .mode $chan +o $2- }
